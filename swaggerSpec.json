{
  "openapi": "3.0.3",
  "info": {
    "title": "Alkemy OT293",
    "description": "Description",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://localhost:3000/api-docs"
    }
  ],
  "tags": [
    {
      "name": "Members",
      "description": "Everything about ONG members"
    }
  ],
  "paths": {
    "/members": {
      "get": {
        "tags": [
          "Members"
        ],
        "summary": "Get all members.",
        "parameters": [
          {
            "name": "token",
            "in": "header",
            "required": true,
            "description": "Admin token.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "page",
            "in": "query",
            "description": "Get members by page.",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "operationId": "findAllMembers",
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "count": {
                      "type": "integer",
                      "example": 123
                    },
                    "rows": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/Member"
                      }
                    },
                    "previousPageUrl": {
                      "type": "string"
                    },
                    "nextPageUrl": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "403": {
            "description": "You are not authorized to access this resource."
          },
          "404": {
            "description": "Not found"
          }
        },
        "security": [
          {
            "bearer_auth": []
          }
        ]
      },
      "post": {
        "tags": [
          "Members"
        ],
        "summary": "Create a new member.",
        "parameters": [
          {
            "name": "token",
            "in": "header",
            "required": true,
            "description": "Admin token.",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "description": "Update an existent member.",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Member"
              }
            },
            "application/x-www-form-urlencoded": {
              "schema": {
                "$ref": "#/components/schemas/Member"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Miembro creado correctamente"
          },
          "403": {
            "description": "You are not authorized to access this resource."
          },
          "404": {
            "description": "Campos obligatorios."
          }
        },
        "security": [
          {
            "bearer_auth": []
          }
        ]
      }
    },
    "/members/{id}": {
      "put": {
        "tags": [
          "Members"
        ],
        "summary": "Edit a member by id.",
        "parameters": [
          {
            "name": "token",
            "in": "header",
            "required": true,
            "description": "Admin token.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "id",
            "in": "path",
            "description": "Member Id",
            "required": true,
            "schema": {
              "type": "integer",
              "example": 4
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Miembro Actualizado con exito"
                    },
                    "data": {
                      "type": "string",
                      "example": [
                        1,
                        4
                      ]
                    }
                  }
                }
              }
            }
          },
          "403": {
            "description": "You are not authorized to access this resource."
          },
          "404": {
            "description": "Miembro no existe."
          }
        },
        "security": [
          {
            "bearer_auth": []
          }
        ]
      },
      "delete": {
        "tags": [
          "Members"
        ],
        "summary": "Deletes a member",
        "parameters": [
          {
            "name": "token",
            "in": "header",
            "required": true,
            "description": "Admin token.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "id",
            "in": "path",
            "description": "Member Id",
            "required": true,
            "schema": {
              "type": "integer",
              "example": 4
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "deleted": {
                      "type": "boolean",
                      "example": true
                    }
                  }
                }
              }
            }
          },
          "403": {
            "description": "You are not authorized to access this resource."
          },
          "404": {
            "description": "Member not found."
          }
        },
        "security": [
          {
            "bearer_auth": []
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "Member": {
        "required": [
          "name",
          "image"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64",
            "example": 10
          },
          "name": {
            "type": "string",
            "example": "Martin Perez"
          },
          "image": {
            "type": "string"
          },
          "facebookUrl": {
            "type": "string",
            "example": "fb.com/MartinPerez"
          },
          "instagramUrl": {
            "type": "string",
            "example": "ig.com/MartinPerez"
          },
          "linkedinUrl": {
            "type": "string",
            "example": "linkedin.com/Martinperez"
          }
        }
      }
    },
    "requestBodies": {
      "Member": {
        "description": "Member object",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/Member"
            }
          },
          "application/xml": {
            "schema": {
              "$ref": "#/components/schemas/Member"
            }
          }
        }
      }
    },
    "securitySchemes": {
      "bearer_auth": {
        "type": "http",
        "scheme": "bearer"
      }
    }
  }
}